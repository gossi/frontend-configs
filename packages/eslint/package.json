{
  "name": "@gossi/config-eslint",
  "version": "1.3.2",
  "repository": "https://github.com/gossi/frontend-configs",
  "type": "module",
  "files": [
    "src"
  ],
  "exports": {
    ".": "./src/index.js",
    "./base": "./src/configs/base.js",
    "./ember": "./src/configs/ember.js",
    "./json": "./src/configs/json.js",
    "./node": "./src/configs/node.js"
  },
  "scripts": {
    "lint": "pnpm run '/^lint:(?!.*(fix)).*$/'",
    "lint:fix": "pnpm run '/^lint:[^:]*:fix/'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix"
  },
  "dependencies": {
    "@eslint/js": "catalog:lint",
    "@stylistic/eslint-plugin": "catalog:lint",
    "eslint-config-prettier": "catalog:lint",
    "eslint-plugin-decorator-position": "catalog:lint",
    "eslint-plugin-import-x": "catalog:lint",
    "eslint-import-resolver-typescript": "catalog:lint",
    "eslint-plugin-json": "catalog:lint",
    "eslint-plugin-n": "catalog:lint",
    "eslint-plugin-prettier": "catalog:lint",
    "eslint-plugin-simple-import-sort": "catalog:lint",
    "eslint-plugin-unicorn": "catalog:lint",
    "globals": "catalog:lib",
    "typescript-eslint": "catalog:lint"
  },
  "peerDependencies": {
    "@babel/eslint-parser": "^7.22.5",
    "@babel/plugin-proposal-decorators": "^7.22.5",
    "eslint": "^9.0.0 ",
    "eslint-plugin-ember": "^12.0.0",
    "eslint-plugin-qunit": "^8.0.0"
  },
  "peerDependenciesMeta": {
    "@babel/eslint-parser": {
      "optional": true
    },
    "@babel/plugin-proposal-decorators": {
      "optional": true
    },
    "eslint-plugin-ember": {
      "optional": true
    },
    "eslint-plugin-qunit": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/eslint-parser": "catalog:build",
    "@babel/plugin-proposal-decorators": "catalog:build",
    "@babel/preset-env": "catalog:build",
    "eslint": "catalog:lint",
    "eslint-plugin-ember": "catalog:lint",
    "eslint-plugin-qunit": "catalog:lint",
    "prettier": "catalog:lint",
    "typescript": "catalog:build"
  },
  "engines": {
    "node": "^20.11.0 || >=22"
  }
}

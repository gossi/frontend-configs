{
  "name": "ember-app-vite",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for ember-app-vite goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "type": "module",
  "imports": {
    "#/*": "./src/*",
    "#config": "./src/config.ts",
    "#components/*": "./src/components/*",
    "#tests/*": "./tests/*"
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "scripts": {
    "build": "vite build",
    "lint": "pnpm run '/^lint:(?!.*(fix)).*$/'",
    "lint:fix": "pnpm run '/^lint:[^:]*:fix/'",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "dev": "vite",
    "test": "vite build --mode test && testem ci --port 0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/plugin-transform-typescript": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@ember/app-tsconfig": "^2.0.0",
    "@ember/optional-features": "^2.3.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.4.1",
    "@ember/test-waiters": "^4.1.0",
    "@embroider/macros": "^1.19.5",
    "@embroider/core": "^4.4.1",
    "@embroider/vite": "^1.4.4",
    "@embroider/compat": "^4.1.11",
    "@embroider/router": "^3.0.6",
    "@embroider/config-meta-loader": "^1.0.0",
    "@eslint/js": "^9.39.2",
    "@glimmer/component": "^2.0.0",
    "@glint/core": "^2.0.0-alpha.2",
    "@glint/environment-ember-loose": "^2.0.0-alpha.3",
    "@glint/environment-ember-template-imports": "^2.0.0-alpha.3",
    "@glint/template": "^1.7.3",
    "@gossi/config-eslint": "workspace:*",
    "@gossi/config-prettier": "workspace:*",
    "@gossi/config-stylelint": "workspace:*",
    "@gossi/config-template-lint": "workspace:*",
    "@rollup/plugin-babel": "^6.1.0",
    "@storybook/types": "^8.6.14",
    "@types/qunit": "^2.19.13",
    "@types/rsvp": "^4.0.9",
    "babel-plugin-ember-template-compilation": "^3.0.1",
    "decorator-transforms": "^2.3.0",
    "ember-auto-import": "^2.12.0",
    "ember-cli": "~6.8.1",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-deprecation-workflow": "^3.4.0",
    "ember-cli-htmlbars": "^7.0.0",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.2.2",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.1.1",
    "ember-source": "~6.9.0",
    "ember-template-lint": "^7.9.3",
    "ember-welcome-page": "^8.0.4",
    "eslint": "^9.39.1",
    "eslint-plugin-ember": "^12.7.4",
    "eslint-plugin-qunit": "^8.2.5",
    "eslint-plugin-storybook": "^10.0.8",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "qunit": "^2.24.2",
    "qunit-dom": "^3.5.0",
    "storybook": "^10.1.4",
    "stylelint": "^16.26.1",
    "testem": "^3.16.0",

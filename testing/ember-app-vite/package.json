{
  "name": "ember-app-vite",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for ember-app-vite goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "type": "module",
  "imports": {
    "#/*": "./src/*",
    "#config": "./src/config.ts",
    "#components/*": "./src/components/*",
    "#tests/*": "./tests/*"
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "scripts": {
    "build": "vite build",
    "lint": "pnpm run '/^lint:(?!.*(fix)).*$/'",
    "lint:fix": "pnpm run '/^lint:[^:]*:fix/'",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "dev": "vite",
    "test": "vite build --mode test && testem ci --port 0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.7",
    "@babel/runtime": "^7.27.6",
    "@babel/plugin-transform-runtime": "^7.27.4",
    "@babel/plugin-transform-typescript": "^7.27.1",
    "@babel/eslint-parser": "^7.27.5",
    "@ember/app-tsconfig": "^1.0.3",
    "@ember/optional-features": "^2.2.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.2.2",
    "@ember/test-waiters": "^4.1.0",
    "@embroider/macros": "^1.18.0",
    "@embroider/core": "^4.1.1",
    "@embroider/vite": "^1.1.5",
    "@embroider/compat": "^4.1.0",
    "@embroider/router": "^3.0.1",
    "@embroider/config-meta-loader": "^1.0.0",
    "@eslint/js": "^9.30.0",
    "@glimmer/component": "^2.0.0",
    "@glint/core": "^2.0.0-alpha.2",
    "@glint/environment-ember-loose": "^2.0.0-alpha.2",
    "@glint/environment-ember-template-imports": "^2.0.0-alpha.2",
    "@glint/template": "^1.6.0-alpha.1",
    "@gossi/config-eslint": "workspace:*",
    "@gossi/config-prettier": "workspace:*",
    "@gossi/config-stylelint": "workspace:*",
    "@gossi/config-template-lint": "workspace:*",
    "@rollup/plugin-babel": "^6.0.4",
    "@storybook/types": "^8.6.14",
    "@types/qunit": "^2.19.12",
    "@types/rsvp": "^4.0.9",
    "babel-plugin-ember-template-compilation": "^3.0.0",
    "decorator-transforms": "^2.3.0",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~6.5.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-deprecation-workflow": "^3.3.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.2.2",
    "ember-page-title": "^9.0.2",
    "ember-qunit": "^9.0.3",
    "ember-resolver": "^13.1.1",
    "ember-source": "~6.5.0",
    "ember-template-lint": "^7.9.1",
    "ember-welcome-page": "^7.0.2",
    "eslint": "^9.27.0",
    "eslint-plugin-ember": "^12.5.0",
    "eslint-plugin-qunit": "^8.1.2",
    "eslint-plugin-storybook": "^9.0.12",
    "globals": "^16.2.0",
    "prettier": "^3.6.0",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "storybook": "^9.0.14",
    "stylelint": "^16.21.0",

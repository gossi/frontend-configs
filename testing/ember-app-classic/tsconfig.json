{
  "extends": "@ember/app-tsconfig",
  "include": [
    "app",
    "tests",
    "types/**/*"
  ],
  "compilerOptions": {
    "declaration": false,
    "declarationMap": false,
    "baseUrl": ".",
    "paths": {
      "ember-app-classic/tests/*": [
        "tests/*"
      ],
      "ember-app-classic/*": [
        "app/*"
      ],
      "*": [
        "types/*"
      ]
    },
    "types": [
      "ember-source/types"
    ]
  }
}

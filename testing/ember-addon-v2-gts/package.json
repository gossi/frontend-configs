{
  "name": "ember-addon-v2-ts",
  "version": "0.0.0",
  "private": true,
  "description": "The default blueprint for Embroider v2 addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "files": [
    "addon-main.cjs",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint": "concurrently 'npm:lint:*(!fix)'",
    "lint:fix": "concurrently 'npm:lint:*:fix'",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "start": "rollup --config --watch"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.5"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.22.5",
    "@babel/plugin-syntax-decorators": "^7.22.5",
    "@embroider/addon-dev": "^3.1.1",
    "@glint/core": "^1.0.2",
    "@glint/environment-ember-loose": "^1.0.2",
    "@glint/environment-ember-template-imports": "^1.0.2",
    "@glint/template": "^1.0.2",
    "@glimmer/component": "^1.1.2",
    "@gossi/config-eslint": "workspace:*",
    "@gossi/config-prettier": "workspace:*",
    "@gossi/config-targets": "workspace:*",
    "@gossi/config-template-lint": "workspace:*",
    "@rollup/plugin-babel": "^6.0.3",
    "@tsconfig/ember": "^2.0.0",
    "concurrently": "^8.2.0",
    "ember-modifier": "^4.1.0",
    "ember-source": "^4.12.0",
    "ember-template-lint": "^5.10.1",
    "ember-cli-htmlbars": "^6.2.0",
    "eslint": "^8.42.0",
    "prettier": "^2.8.8",
    "rollup": "^3.25.0",
    "rollup-plugin-glimmer-template-tag": "^0.4.1",
    "typescript": "^5.1.3",
    "webpack": "^5.86.0"
  },
  "peerDependencies": {
    "@glimmer/component": "^1.1.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">= v18.15.0"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./addon-main.js": "./addon-main.cjs"
  }
}

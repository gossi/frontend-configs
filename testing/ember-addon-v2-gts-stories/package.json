{
  "name": "ember-addon-v2-gts-stories",
  "private": true,
  "description": "The default blueprint for Embroider v2 addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "files": [
    "addon-main.cjs",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint": "concurrently 'npm:lint:*(!fix)'",
    "lint:fix": "concurrently 'npm:lint:*:fix'",
    "lint:css": "stylelint 'src/**/*.css' --allow-empty-input",
    "lint:css:fix": "stylelint 'src/**/*.css' --allow-empty-input --fix",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "start": "rollup --config --watch"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.9"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.25.9",
    "@babel/plugin-syntax-decorators": "7.25.9",
    "@embroider/addon-dev": "7.1.1",
    "@glint/core": "1.5.1",
    "@glint/environment-ember-loose": "1.5.1",
    "@glint/environment-ember-template-imports": "1.5.1",
    "@glint/template": "1.5.1",
    "@glimmer/component": "2.0.0",
    "@gossi/config-eslint": "workspace:*",
    "@gossi/config-prettier": "workspace:*",
    "@gossi/config-stylelint": "workspace:*",
    "@gossi/config-targets": "workspace:*",
    "@gossi/config-template-lint": "workspace:*",
    "@rollup/plugin-babel": "6.0.4",
    "@storybook/types": "8.5.0",
    "@tsconfig/ember": "3.0.8",
    "@types/react": "^18.3.18",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "concurrently": "^9.1.2",
    "ember-modifier": "4.2.0",
    "ember-source": "6.1.0",
    "ember-template-lint": "^6.0.0",
    "ember-cli-htmlbars": "6.3.0",
    "eslint": "^8.57.1",
    "eslint-plugin-ember": "^12.3.3",
    "eslint-plugin-qunit": "^8.1.2",
    "eslint-plugin-storybook": "^0.11.1",
    "prettier": "^3.4.2",
    "rollup": "4.31.0",
    "rollup-plugin-glimmer-template-tag": "0.4.1",
    "stylelint": "16.13.2",
    "typescript": "5.7.3",
    "webpack": "5.97.1"
  },
  "peerDependencies": {
    "@glimmer/component": "^2.0.0"
  },
  "engines": {
    "node": ">= 20.*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./addon-main.js": "./addon-main.cjs"
  }
}

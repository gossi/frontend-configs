{
  "name": "ember-addon-v2",
  "private": true,
  "description": "The default blueprint for Embroider v2 addons.",
  "keywords": [
    "ember-addon-v2"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "files": [
    "addon-main.cjs",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint": "concurrently 'npm:lint:*(!fix)'",
    "lint:fix": "concurrently 'npm:lint:*:fix'",
    "lint:css": "stylelint 'src/**/*.css' --allow-empty-input",
    "lint:css:fix": "stylelint 'src/**/*.css' --allow-empty-input --fix",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "start": "rollup --config --watch"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.9",
    "decorator-transforms": "^2.2.2"
  },
  "devDependencies": {
    "@babel/core": "7.27.4",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.27.1",
    "@babel/plugin-syntax-decorators": "7.27.1",
    "@ember/library-tsconfig": "^1.0.0",
    "@embroider/addon-dev": "8.1.0",
    "@embroider/core": "^4.1.0",
    "@embroider/compat": "^4.1.0",
    "@embroider/macros": "^1.18.0",
    "@glimmer/component": "^2.0.0",
    "@glint/core": "^2.0.0-alpha.2",
    "@glint/environment-ember-loose": "^2.0.0-alpha.2",
    "@glint/environment-ember-template-imports": "^2.0.0-alpha.2",
    "@glint/template": "^1.6.0-alpha.1",
    "@gossi/config-eslint": "workspace:*",
    "@gossi/config-prettier": "workspace:*",
    "@gossi/config-stylelint": "workspace:*",
    "@gossi/config-targets": "workspace:*",
    "@gossi/config-template-lint": "workspace:*",
    "@rollup/plugin-babel": "6.0.4",
    "@storybook/types": "^8.6.14",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "concurrently": "^9.1.2",
    "ember-modifier": "4.2.2",
    "ember-source": "6.5.0",
    "ember-template-lint": "^7.9.1",
    "ember-cli-htmlbars": "6.3.0",
    "eslint": "^9.29.0",
    "eslint-plugin-ember": "^12.5.0",
    "eslint-plugin-qunit": "^8.1.2",
    "eslint-plugin-storybook": "^9.0.11",
    "prettier": "^3.5.3",
    "rollup": "4.43.0",
    "storybook": "^9.0.11",
    "stylelint": "16.20.0",
    "typescript": "5.8.3",
    "webpack": "5.99.9"
  },
  "engines": {
    "node": ">= 20.11.*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon-v2": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./addon-main.js": "./addon-main.cjs"
  }
}
